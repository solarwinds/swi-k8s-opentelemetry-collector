{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "otel": {
      "type": "object",
      "properties": {
        "endpoint": {
          "type": "string"
        }
      },
      "required": ["endpoint"],
      "additionalProperties": false
    },
    "cluster": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "uid": {
          "type": "string"
        }
      },
      "required": ["name", "uid"],
      "additionalProperties": false
    }
  },
  "required": ["otel", "cluster"],
  "additionalProperties": false
}
