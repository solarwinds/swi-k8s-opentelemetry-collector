              scheme: http
              scrape_interval: 60s
              static_configs:
              - targets:
                - ${KUBE_STATE_METRICS_URL}
        prometheus/node-metrics:
          config:
            scrape_configs:
            - authorization:
                credentials_file: /var/run/secrets/kubernetes.io/serviceaccount/token
