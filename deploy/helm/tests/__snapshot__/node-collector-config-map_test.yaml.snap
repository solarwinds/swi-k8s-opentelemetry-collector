              "^(https?://)?[a-zA-Z0-9][-a-zA-Z0-9]*\\.[a-zA-Z0-9][-a-zA-Z0-9\\.]*(:\\d+)?$")
              and not(IsMatch(datapoint.attributes["destination_service"], ".*\\.cluster\\.local$"))
