            operations:
            - action: aggregate_labels
              aggregation_type: sum
              label_set:
              - pod
              - namespace
              - k8s.node.name
          - action: insert
            experimental_match_labels:
              k8s.node.name: \S+
              namespace: \S+
              pod: \S+
            include: k8s.container_network_transmit_packets_dropped_total
            match_type: regexp
            new_name: k8s.pod.network.transmit_packets_dropped
            operations:
            - action: aggregate_labels
              aggregation_type: sum
              label_set:
              - pod
              - namespace
              - k8s.node.name
          - action: insert
            experimental_match_labels:
              k8s.node.name: \S+
              namespace: \S+
              pod: \S+
            include: k8s.container_fs_reads_total
            match_type: regexp
            new_name: k8s.pod.fs.reads.rate
            operations:
            - action: aggregate_labels
              aggregation_type: sum
              label_set:
              - pod
              - namespace
              - k8s.node.name
          - action: insert
            experimental_match_labels:
              k8s.node.name: \S+
              namespace: \S+
              pod: \S+
            include: k8s.container_fs_writes_total
            match_type: regexp
