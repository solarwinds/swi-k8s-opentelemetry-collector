Reducer spec should match snapshot when using default values:
  1: |
    containers:
      - args:
          - --port=7000
          - --log-console
          - --no-log-file
          - --warning
          - --enable-aws-enrichment
          - --disable-prometheus-metrics
          - --enable-otlp-grpc-metrics
          - --otlp-grpc-metrics-host=RELEASE-NAME-swo-k8s-collector-metrics-collector
          - --otlp-grpc-metrics-port=4317
          - --num-ingest-shards=3
          - --num-matching-shards=3
          - --num-aggregation-shards=3
        env:
          - name: EBPF_NET_CRASH_METRIC_HOST
            value: RELEASE-NAME-swo-k8s-collector-metrics-collector
          - name: EBPF_NET_CRASH_METRIC_PORT
            value: "4317"
        image: otel/opentelemetry-ebpf-reducer:v0.10.0
        imagePullPolicy: IfNotPresent
        name: reducer
        ports:
          - containerPort: 7000
            name: telemetry
            protocol: TCP
        readinessProbe:
          exec:
            command:
              - /srv/health_check.sh
              - readiness_probe
              - localhost
              - "7000"
          initialDelaySeconds: 5
          periodSeconds: 20
          timeoutSeconds: 5
        resources: null
        volumeMounts: null
    initContainers:
      - command:
          - sh
          - -c
          - until nc -zv $METRICS_COLLECTOR_HOST $METRICS_COLLECTOR_PORT; do echo "Waiting for metrics collector endpoint..."; sleep 5; done;
        env:
          - name: METRICS_COLLECTOR_HOST
            value: RELEASE-NAME-swo-k8s-collector-metrics-collector
          - name: METRICS_COLLECTOR_PORT
            value: "4317"
        image: busybox:1.36.1@sha256:5cd3db04b8be5773388576a83177aff4f40a03457a63855f4b9cbe30542b9a43
        imagePullPolicy: IfNotPresent
        name: wait-for-metrics-collector
    nodeSelector:
      kubernetes.io/arch: amd64
      kubernetes.io/os: linux
    volumes: null
