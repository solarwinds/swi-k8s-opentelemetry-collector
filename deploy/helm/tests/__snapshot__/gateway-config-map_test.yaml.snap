          - set(resource.attributes["sw.k8s.dst.k8s.daemonset.name"], resource.attributes["sw.k8s.dst.workload.name"])
            where resource.attributes["sw.k8s.dst.workload.type"] == "DaemonSet" and resource.attributes["sw.k8s.dst.workload.name"]
            != nil
          - set(resource.attributes["sw.k8s.dst.k8s.service.name"], resource.attributes["sw.k8s.dst.workload.name"])
            where resource.attributes["sw.k8s.dst.workload.type"] == "Service" and resource.attributes["sw.k8s.dst.workload.name"]
            != nil
          - set(resource.attributes["sw.k8s.dst.k8s.job.name"], resource.attributes["sw.k8s.dst.workload.name"])
            where resource.attributes["sw.k8s.dst.workload.type"] == "Job" and resource.attributes["sw.k8s.dst.workload.name"]
            != nil
          - set(resource.attributes["sw.k8s.dst.k8s.node.name"], resource.attributes["sw.k8s.dst.workload.name"])
